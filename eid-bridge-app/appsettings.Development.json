{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "OphtalmoPro.EidBridge": "Debug"
    }
  },
  "AllowedHosts": "localhost;127.0.0.1",
  "EidBridge": {
    "Security": {
      "AllowedOrigins": [
        "https://localhost:5173",
        "https://localhost:3000",
        "http://localhost:5173",
        "http://localhost:3000",
        "http://localhost:8080"
      ],
      "RequireHttps": false
    },
    "Middleware": {
      "TestPorts": [ 53001, 35963, 35964, 24727 ],
      "TimeoutSeconds": 30
    }
  },
  "EnableSwagger": true
}